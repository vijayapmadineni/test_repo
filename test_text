    logger.info("Start: Get all latest claim data")
    adh_refill_df = spark.read.format("jdbc").option("url", cdm_creds['url']).option("user", cdm_creds['username']).option("password", cdm_creds['password']).option("dbtable", adh_refill_qry).option("driver", "org.postgresql.Driver").option("fetchsize", 1000).load()
    adh_refill_df.cache()
    adh_refill_df_cnt = adh_refill_df.count()
    logger.info(f"adh_refill_df_cnt : {adh_refill_df_cnt}")
    logger.info("End: Get all latest claim data")

    logger.info("Start: Get all all members who used insulin in this year")
    insln_mem_df = spark.read.format("jdbc").option("url", cdm_creds['url']).option("user", cdm_creds['username']).option("password", cdm_creds['password']).option("dbtable", insln_mem_qry).option("driver", "org.postgresql.Driver").option("fetchsize", 1000).load()
    insln_mem_df_cnt = insln_mem_df.count()
    logger.info(f"insln_mem_df_cnt : {insln_mem_df_cnt}")
    logger.info("End: Get all all members who used insulin in this year")
